{
  "name": "kronos/redis-session-handler",
  "description": "Port of https://github.com/snc/SncRedisBundle RedisSessionHandler",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Eric Durand-Tremblay",
      "email": "etremblay@kronostechnologies.com"
    }
  ],
  "require-dev": {
    "vimeo/psalm": "6.0.0",
    "cyclonedx/cyclonedx-php-composer": "5.2.0",
    "squizlabs/php_codesniffer": "3.11.3",
    "dealerdirect/phpcodesniffer-composer-installer": "1.0.0",
    "kronostechnologies/php-coding-standard": "2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Kronos\\Session\\": "Kronos/Session"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-redis": "*"
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "cyclonedx/cyclonedx-php-composer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
